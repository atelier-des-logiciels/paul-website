{
  "name": "paul-website",
  "version": "1.0.0",
  "description": "Paul website",
  "main": "index.js",
  "scripts": {
    "start:dev": "webpack serve --config internals/webpack/common.js --config internals/webpack/dev.js -m",
    "build:dev": "webpack build --config internals/webpack/common.js --config internals/webpack/dev.js -m",
    "build:prod": "webpack build --config internals/webpack/common.js --config internals/webpack/prod.js -m",
    "serve:prod": "http-server dist -p 3000",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "prettier --write 'src/**/*.{ts,tsx}'",
    "test": "jest --config=internals/jest/jest.config.js",
    "jest-preview": "jest-preview",
    "test:watch": "npm test -- --watch",
    "test:coverage": "npm test -- --coverage",
    "test:types": "npx tsc --project ./tsconfig.json --noEmit",
    "test:all": "npm run test && npm run test:types",
    "audit:security": "npm audit --production --audit-level=high",
    "audit:security:alldeps": "npm audit --audit-level=high",
    "plop": "plop",
    "tsc": "tsc"
  },
  "author": "paul@musso.io",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.22.17",
